{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "fsdocs",
                "build",
                "--clean",
                "--eval",
                "--input",
                "src",
                "--output",
                "docs",
                "--parameters",
                "root",
                "/"
            ],
            "group": "build",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "fsdocs",
                "watch",
                "--eval",
                "--input",
                "src",
                "--output",
                "docs",
                "--parameters",
                "root",
                "/"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}